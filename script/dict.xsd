<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	   xmlns:xml="http://www.w3.org/XML/1998/namespace">

<xs:import namespace="http://www.w3.org/XML/1998/namespace"
	   schemaLocation="xml.xsd"/>


<xs:element name="dict">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="entry" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="lemma"/>
            <xs:element ref="stem"/>
            <xs:element ref="contlex"/>
            <xs:element ref="pos"/>
	    <xs:element name="article" maxOccurs="unbounded">
	      <xs:complexType>
		<xs:sequence>
                  <xs:choice maxOccurs="unbounded" minOccurs="2">
		    <xs:element ref="der"/>
		    <xs:element ref="dial"/>
		    <xs:element ref="acron"/>
		    <xs:element ref="CONN"/>
		    <xs:element ref="source"/>
		    <xs:element ref="SPELL"/>
		    <xs:element ref="abbr"/>
		    <xs:element ref="syn"/>
		    <xs:element ref="eg"/>
		    <xs:element ref="de"/>
		    <xs:element ref="eng"/>
		    <xs:element ref="usage"/>
		    <xs:element ref="dom"/>
		    <xs:element ref="isa"/>
		    <xs:element ref="fin"/>
		    <xs:element ref="ant"/>
		    <xs:element ref="cf"/>
		    <xs:element ref="msp"/>
		    <xs:element ref="pro"/>
		    <xs:element ref="sci"/>
		    <xs:element ref="see"/>
		    <xs:element ref="sem"/>
		    <xs:element ref="syno"/>
		  </xs:choice>
		</xs:sequence>
<!--		<xs:attribute ref="RN"/>
-->	      </xs:complexType>
	    </xs:element>
          </xs:sequence>
<!--	  <xs:attribute name="exclude">
	    <xs:simpleType>
              <xs:restriction base="xs:string">
		<xs:enumeration value="fst"/>
              </xs:restriction>
	    </xs:simpleType>
	  </xs:attribute>
-->        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>


<xs:element name="eng" type="lang_Type"/>
<xs:element name="de" type="lang_Type"/><!-- GERMAN -->
<xs:element name="fin" type="lang_Type"/>
<xs:element name="der" type="lang_Type"/>
<xs:element name="dial" type="lang_Type"/><!-- -->
<xs:element name="see" type="lang_Type"/> <!-- convert to assoc/assocType@'see' -->
<xs:element name="cf" type="lang_Type"/> <!-- convert to assoc/assocType@'cf' -->
<xs:element name="pro" type="lang_Type"/> <!-- convert to assoc/assocType@'pro' -->


<xs:complexType name="lang_Type">
  <xs:sequence>
    <xs:element name="choice">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="variant" maxOccurs="unbounded">
	    <xs:complexType>
	      <xs:simpleContent>
		<xs:extension base="xs:string">
		  <xs:attribute name="lang">
		    <xs:simpleType>
		      <xs:restriction base="xs:string">
			<xs:enumeration value="en_US"/>
			<xs:enumeration value="en_UK"/>
		      </xs:restriction>
		    </xs:simpleType>
		  </xs:attribute>
		</xs:extension>
	      </xs:simpleContent>
	    </xs:complexType>
	  </xs:element>
	</xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:sequence>
</xs:complexType>

<xs:element name="eg">
<xs:complexType>
  <xs:sequence>
    <xs:element name="komi" type="xs:string"/>
    <xs:element ref="eng"/>
    <xs:element ref="fin"/>
  </xs:sequence>
</xs:complexType>
</xs:element>


<xs:element name="acron" type="xs:string"/><!--long form of acronym -->
<xs:element name="syn" type="xs:string"/> <!-- syntax and colocation-->
<xs:element name="abbr" type="xs:string"/> <!-- allegro form-->
<xs:element name="SPELL" type="xs:string"/> <!-- spelling variant perhaps one that is prefered -->
<xs:element name="lemma" type="xs:string"/> <!-- -->
<xs:element name="stem" type="xs:string"/> <!-- -->
<xs:element name="contlex" type="xs:string"/> <!-- -->
<xs:element name="sci" type="xs:string"/> <!-- scientific term -->
<xs:element name="syno" type="xs:string"/> <!-- convert to assoc/assocType@'synonym' -->
<xs:element name="ant" type="xs:string"/> <!-- convert to assoc/assocType@'ant' -->

